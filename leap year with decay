#include<stdio.h>
int main(){
int n,l=0,y=0;
scanf("%d",&n);
if(n%4==0 && n%100!=0 || n%400==0){
printf("leap year\n");
}
else{
printf("not\n");
}
for(int i=1;i<=10;i++){
if(n%4==0 && n%100!=0 || n%400==0){
l++;
}
else{
y++;
}
n++;

}
printf("leap count = %d\n",l);
printf("non leap count = %d",y);
}

